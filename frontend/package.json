{
  "name": "actix-web-rest-api-frontend",
  "version": "1.0.0",
  "description": "TypeScript/Bun/React frontend for Actix Web REST API with JWT authentication and multi-tenant support",
  "scripts": {
    "dev": "bun --hot src/main.tsx",
    "build": "bun build src/main.tsx --outdir dist --minify",
    "preview": "bun run dist/main.js",
    "test": "bun test --coverage",
    "test:watch": "bun test --watch",
    "test:ui": "bun test --dom"
  },
  "keywords": [
    "typescript",
    "bun",
    "react",
    "frontend",
    "jwt",
    "multi-tenant"
  ],
  "author": "Actix Web REST API Team",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "typescript": "^5.9.0"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.0.0",
    "react-hook-form": "^7.0.0"
  }
}
